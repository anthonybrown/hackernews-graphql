{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["warn", {
      "printWidth": 100,
      "singleQuote": true,
      "trailingComma": "all",
    }],
    "import/prefer-default-export": 0
  },
  "env": {
    "node": true,
    "jest": true
  },
  "plugins": [
    "prettier"
  ]
}
